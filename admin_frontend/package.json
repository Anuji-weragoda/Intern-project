{
  "name": "admin-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --color",
    "test:watch": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --color --watch",
    "test:coverage": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --color --coverage",
    "allure:generate": "allure generate ./allure-results --clean -o ./allure-report",
    "allure:open": "allure open ./allure-report",
    "allure:serve": "allure serve ./allure-results",
    "allure:fresh": "powershell -NoProfile -Command \"if (Test-Path .\\allure-results) { Remove-Item -Recurse -Force .\\allure-results }; if (Test-Path .\\allure-report) { Remove-Item -Recurse -Force .\\allure-report }; node --experimental-vm-modules .\\node_modules\\jest\\bin\\jest.js --color --runInBand; allure generate .\\allure-results --clean -o .\\allure-report; allure open .\\allure-report\""
  },
  "dependencies": {
    "date-fns": "^4.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@testing-library/jest-dom": "^6.9.1",
  "@testing-library/react": "^16.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
  "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
  "allure-commandline": "^2.34.1",
  "allure-jest": "^3.4.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "ts-jest": "^29.4.5",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7"
  }
}
